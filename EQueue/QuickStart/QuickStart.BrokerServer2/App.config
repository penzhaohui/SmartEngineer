<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <runtime>
    <gcServer enabled="true"/>
    <gcConcurrent enabled="true"/>
  </runtime>
  <appSettings>
    <add key="nameServerAddress" value=""/>
    <add key="brokerName" value="Broker2"/>
    <add key="groupName" value="BrokerGroup2"/>
    <add key="producerPort" value="6000"/>
    <add key="consumerPort" value="6001"/>
    <add key="adminPort" value="6002"/>
    <add key="fileStoreRootPath" value="d:\equeue-store2"/>
    <add key="isMemoryMode" value="false"/>
    <add key="syncFlush" value="false"/>
    <add key="deleteMessageIgnoreUnConsumed" value="false"/>
    <add key="notifyWhenMessageArrived" value="true"/>
    <add key="chunkSize" value="256"/><!--MB-->
    <add key="chunkWriteBuffer" value="256"/><!--KB-->
    <add key="enableCache" value="true"/>
    <add key="flushInterval" value="100"/><!--Milliseconds-->
    <add key="chunkCacheMinCount" value="1"/>
    <add key="chunkCacheMaxCount" value="2"/>
    <add key="messageWriteQueueThreshold" value="10000"/>
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/></startup></configuration>
